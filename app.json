{
  "expo": {
    "name": "ChurrasApp",
    "slug": "ChurrasApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": ""
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": ""
      }
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "plugins": [
      [ 
        "expo-font",
        {
          "fonts": ["./assets/fonts/CintraRegular.ttf"]
        }
      ]
    ]
  }
}
